{
	"manifest_version": 2,

  	"name": "Momentum Dash",
	"description": "A momentum dash clone",
	"version": "1.0",
	"icons": {
		"16": "img/logo 16.png",
		"24": "img/logo 24.png",
		"36": "img/logo 36.png",
		"48": "img/logo 48.png"
	},

  	"browser_action": {
  		"default_icon": {
			"16": "img/logo 16.png",
			"24": "img/logo 24.png",
			"36": "img/logo 36.png",
			"48": "img/logo 48.png"
		}
	  },
	"permissions": [
		"tabs",
		"activeTab",
		"history",
		"storage",
		"geolocation"
	],
	"background": {
		"scripts": [
			"background.js",
			"external/jquery-3.2.1.js"
		]
	}
}
