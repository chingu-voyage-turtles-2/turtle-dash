{
	"manifest_version": 2,

  	"name": "Turtle Dash",
	"description": "Transform your empty chrome tabs into a gorgeous, productivity powerhouse.",
	"version": "1.21",
	"icons": {
		"16": "img/logo/logo 16.png",
		"24": "img/logo/logo 24.png",
		"36": "img/logo/logo 36.png",
		"48": "img/logo/logo 48.png",
		"64": "img/logo/logo 64.png",
		"128": "img/logo/logo 128.png",
		"256": "img/logo/logo 256.png"
	},

  	"browser_action": {
  		"default_icon": {
			"16": "img/logo/logo 16.png",
			"24": "img/logo/logo 24.png",
			"36": "img/logo/logo 36.png",
			"48": "img/logo/logo 48.png",
			"64": "img/logo/logo 64.png",
			"128": "img/logo/logo 128.png",
			"256": "img/logo/logo 256.png"
		}
	  },
	"permissions": [
		"tabs",
		"activeTab",
		"history",
		"storage",
		"geolocation"
	],
	"background": {
		"scripts": [
			"background.js",
			"external/jquery-3.2.1.js"
		]
	}
}
